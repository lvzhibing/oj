#include <iostream>

using namespace std;

int main()
{
	float * len;
	float tmp;
	int i = 0, num = 1;
	while(cin>>tmp){
		if(tmp == 0){
			break;
		}
		len[i] = new float(tmp);
		cout<<len[i]<<endl;
		++i;
		++num;
	}

	float l = 0;
	for( int j = 0; j < num; ++j){
		for( int n = 2; l >= len[i]; ++n){
			l = 1 / 2 + l;
		}
		cout << l <<endl;
	}

	return 0;
}
	
